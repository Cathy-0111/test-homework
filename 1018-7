#include<stdio.h>

int main(){
    int n,m=2,t=1,prime=0;
    scanf("%d",&n);
    while(m<n/2+1){
        if(n%m==0){
            prime=0;
            printf("NO");
            break;
        }
        t++;
        m++;
        prime=1;
    }
    if(prime==1){
        printf("YES");
    }
    else if(n==1){
        printf("NO");
    }
}
